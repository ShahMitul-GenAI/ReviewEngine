    "# Developing Summary of Reviews for Each Web\n",
    "amz_cust_reviews = df[\"review\"]\n",
    "amz_reviews_str = \"\".join(each for  each in amz_cust_reviews)\n",
    "print(len(amz_reviews_str))"
   ]
