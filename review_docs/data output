    "if new_data:\n",
    "    search_query = str(product)   # 'premier protein shake, chocolate'\n",
    "    scraper = AmazonScraper()\n",
    "    reviews = scraper.get_closest_product_reviews(search_query, num_reviews = max_cus, debug=True)\n",
    "    print(reviews)"
